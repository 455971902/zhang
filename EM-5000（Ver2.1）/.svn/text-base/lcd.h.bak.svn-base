#ifndef _LCD_H_
#define _LCD_H_

#include "MSP430x54x.h"
#include "chip.h"
#include "math.h"



#define CS_ON    P4OUT&=~BIT3    //cs信号有效
#define CS_OFF   P4OUT|=BIT3    //cs信号无效

#define RES_ON   P4OUT&=~BIT4    //复位信号有效
#define RES_OFF  P4OUT|=BIT4   //复位信号无效

#define CMD_ON   P4OUT&=~BIT5   //写命令
#define DATA_ON  P4OUT|=BIT5    //写数据

#define W_ON     P4OUT&=~BIT6   //写信号有效
#define R_ON     P4OUT|=BIT6    //读信号有效

#define E_OFF    P4OUT&=~BIT7   //允许信号无效
#define E_ON     P4OUT|=BIT7    //允许信号有效

#define LCD_io   P8OUT          //LCD的io口   


#define HOUR     1
#define MIN      2
#define HOUR_MIN 3

#define FONT_CHAR(value)        //中文字符标识
#define FONT_UNICODE_VAL(value) (value),    //中文字符UNICODED值

typedef struct _unico_font_12x12       //定义unicode码结构体
{
  unsigned int unicode;                //unicode码值
  unsigned char data[24];              //字模数组12x12大小
}Unicode;

/////////////////////////////////////////////////////////////////////////////////

  
  void write_cmd(unsigned char cmd);                   //写命令函数
  void write_data(unsigned char data);                 //写数据函数
  
  //*在液晶的x列，一字节的page页处写一个字节数据，其中8点一页，64行液晶有8页
  void send_byte_pos(unsigned char x,unsigned char page,unsigned char data);

  void init_Lcd(void);        //初始化液晶屏
  void clr_lcd(void);         //清除整个液晶屏

////////////////////////////////////////////////////////////////////
  




  
  void clr_lcd_zone(unsigned char x,  //清除以(x,y)为起点，width为宽，height为
                    unsigned char y,  //高的屏幕区域
                    unsigned char width,
                    unsigned char height);  
  
  
  void clr_point(unsigned char x,unsigned char y);    //清除一点函数，(x,y)坐标
  void draw_point(unsigned char x,unsigned char y);   //描一点函数，(x,y)坐标
  
  void draw_line(unsigned char x,  //从(x,y)处到(end_x,end_y)处描一条线函数
                 unsigned char y,
                 unsigned char end_x,
                 unsigned char end_y);
  
  void draw_rect(unsigned char x, //从(x,y)处描一个宽为width，高为height的矩形框
                 unsigned char y,
                 unsigned char width,
                 unsigned char height);
  
  void clr_rect(unsigned char x,   //清除从(x,y)处清除一个宽为width，高为height
                unsigned char y,   //的矩形区域
                unsigned char width,
                unsigned char height);
  
  void fill_rect(unsigned char x, //从(x,y)处填充一个宽为width，高为height的矩形
                 unsigned char y, //区域
                 unsigned char width,
                 unsigned char height);
  
  void draw_symbol(unsigned char x, //从(x,y)处描一个宽为width，高为height的
                   unsigned char y, //图标，symbol_p为图标数据的指针
                   unsigned char width,
                   unsigned char height,
                   const unsigned char* symbol_p,
                   unsigned char mod);
